{
  "model_config": {
    "model_id": "bone_age_01",
    "model_type": {
      "regression": true,
      "binary_classification": true,
      "provide_reference": false
    }
  },
  "columns": {
    "study_id": "StudyID",
    "model_id": "ModelID",
    "sex": "sex",
    "hospital": "hospital",
    "age": "chronological_age",
    "instrument_type": "machine_type",
    "predictions": {
      "regression_prediction": "predicted_age",
      "classification_prediction": "classification"
    },
    "labels": {
      "regression_label": "label",
      "classification_label": "classification_label"
    },
    "features": [
      "standard_deviation",
      "two_standard_deviations",
      "upper_limit",
      "lower_limit",
      "closest_age"
    ],
    "timestamp": null
  },
  "validation_rules": {
    "sex": {
      "type": "enum",
      "values": ["M", "F"]
    },
    "hospital": {
      "type": "enum",
      "values": [
        "Credit Valley Hospital",
        "Mississauga Hospital",
        "Queensway Hospital"
      ]
    },
    "chronological_age": {
      "type": "range",
      "min": 0,
      "max": 400
    },
    "machine_type": {
      "type": "enum",
      "values": ["machine1", "machine2"]
    },
    "standard_deviation": {
      "type": "range",
      "min": 0,
      "max": 13.05
    },
    "two_standard_deviations": {
      "type": "range",
      "min": 0,
      "max": 26.1
    },
    "upper_limit": {
      "type": "range",
      "min": 0,
      "max": 300
    },
    "lower_limit": {
      "type": "range",
      "min": 0,
      "max": 300
    },
    "closest_age": {
      "type": "range",
      "min": 0,
      "max": 216
    }
  },
  "tests": {
    "data_quality_tests": [
      {
        "name": "num_cols"
      },
      {
        "name": "num_empty_rows"
      },
      {
        "name": "num_duplicated_rows"
      },
      {
        "name": "num_duplicated_cols"
      },
      {
        "name": "col_types"
      },
      {
        "name": "num_missing_values"
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "closest_age",
          "left": 0,
          "right": 216
        }
      },
      {
        "name": "test_col_list",
        "params": {
          "column_name": "hospital",
          "values": [
            "Credit Valley Hospital",
            "Mississauga Hospital",
            "Queensway Hospital"
          ]
        }
      }
    ],
    "data_drift_tests": [
      {
        "name": "num_drifted_cols"
      },
      {
        "name": "share_drifted_cols"
      }
    ],
    "regression_tests": [
      {
        "name": "mae"
      }
    ],
    "classification_tests": [
      {
        "name": "accuracy"
      },
      {
        "name": "precision"
      },
      {
        "name": "recall"
      },
      {
        "name": "f1"
      },
      {
        "name": "tnr"
      },
      {
        "name": "fpr"
      },
      {
        "name": "fnr"
      }
    ]
  },
  "alerts": {
    "enable": true,
    "alert_links": [
      {
        "type": "microsoft teams channel",
        "url": "<TODO>"
      }
    ]
  }
}
