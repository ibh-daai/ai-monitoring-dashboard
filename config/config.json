{
  "model_config": {
    "model_id": "bone_age",
    "model_type": {
      "regression": true,
      "binary_classification": true
    }
  },
  "columns": {
    "study_id": "StudyID",
    "sex": "sex",
    "hospital": "hospital",
    "age": "chronological_age",
    "instrument_type": "machine_type",
    "patient_class": "patient_classification",
    "predictions": {
      "regression_prediction": "predicted_age",
      "classification_prediction": "classification"
    },
    "labels": {
      "regression_label": "label",
      "classification_label": "classification_label"
    },
    "features": [
      "standard_deviation",
      "two_standard_deviations",
      "upper_limit",
      "lower_limit",
      "closest_age"
    ],
    "timestamp": null
  },
  "age_filtering": {
    "filter_type": "custom",
    "custom_ranges": [
      { "min": 0, "max": 120 },
      { "min": 120, "max": 180 },
      { "min": 180, "max": 300 }
    ]
  },
  "tests": {
    "data_quality_tests": [
      { "name": "num_cols" },
      { "name": "num_empty_rows" },
      { "name": "num_duplicated_rows" },
      { "name": "num_duplicated_cols" },
      { "name": "col_types" },
      { "name": "num_missing_values" },
      {
        "name": "test_col_list",
        "params": {
          "column_name": "sex",
          "values": ["M", "F"]
        }
      },
      {
        "name": "test_col_list",
        "params": {
          "column_name": "hospital",
          "values": [
            "Credit Valley Hospital",
            "Mississauga Hospital",
            "Queensway Hospital"
          ]
        }
      },
      {
        "name": "test_col_list",
        "params": {
          "column_name": "patient_classification",
          "values": ["IP", "OP", "ED", "ICU"]
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "chronological_age",
          "left": 0,
          "right": 400
        }
      },
      {
        "name": "test_col_list",
        "params": {
          "column_name": "machine_type",
          "values": ["machine1", "machine2"]
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "standard_deviation",
          "left": 0,
          "right": 13.05
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "two_standard_deviations",
          "left": 0,
          "right": 26.1
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "upper_limit",
          "left": 0,
          "right": 300
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "lower_limit",
          "left": 0,
          "right": 300
        }
      },
      {
        "name": "test_col_range",
        "params": {
          "column_name": "closest_age",
          "left": 0,
          "right": 216
        }
      }
    ],
    "data_drift_tests": [
      { "name": "num_drifted_cols" },
      { "name": "share_drifted_cols" }
    ],
    "regression_tests": [
      { 
        "name": "mae",
        "params": {
          "lte": 9.0
        }
      }
    ],
    "classification_tests": [
      { "name": "accuracy" },
      { "name": "precision" },
      { "name": "recall" },
      { "name": "f1" },
      { "name": "tnr" },
      { "name": "fpr" },
      { "name": "fnr" }
    ]
  },
  "dashboard_panels": [
    {
      "name": "num_cols",
      "type": "bar",
      "size": "half"
    },
    {
      "name": "num_missing_values",
      "type": "line",
      "size": "half"
    },
    {
      "name": "num_empty_rows",
      "type": "line",
      "size": "half"
    },
    {
      "name": "num_drifted_cols",
      "type": "line",
      "size": "half"
    },
    {
      "name": "data_drift",
      "type": "line",
      "size": "half"
    },
    {
      "name": "mae",
      "type": "line",
      "size": "full"
    },
    {
      "name": "precision",
      "type": "line",
      "size": "half"
    },
    {
      "name": "recall",
      "type": "line",
      "size": "half"
    },
    {
      "name": "f1",
      "type": "line",
      "size": "half"
    },
    {
      "name": "tnr",
      "type": "line",
      "size": "half"
    },
    {
      "name": "fpr",
      "type": "line",
      "size": "half"
    },
    {
      "name": "fnr",
      "type": "line",
      "size": "half"
    }
  ],
  "info": {
    "project_name": "Bone Age Prediction",
    "project_description": "Bone Age Prediction project",
    "model_developer": "Trillium Health Partners",
    "contact_name": "John Smith",
    "contact_email": "johnsmith@thp.ca",
    "references": [
      {
        "name": "RSNA Pediatric Bone Age Challenge 2017",
        "url": "https://www.rsna.org/education/ai-resources-and-training/ai-image-challenge/rsna-pediatric-bone-age-challenge-2017"
      }
